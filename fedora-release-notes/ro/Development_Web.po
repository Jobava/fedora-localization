msgstr ""

#. Tag: para
#, no-c-format
msgid ""
"<ulink url=\"https://wiki.php.net/rfc/argument_unpacking\">Argument "
"Unpacking</ulink>"
msgstr ""

#. Tag: para
#, no-c-format
msgid ""
"<ulink url=\"https://wiki.php.net/rfc/variadics\">Syntax for variadic "
"functions</ulink>"
msgstr ""

#. Tag: para
#, no-c-format
msgid ""
"<ulink url=\"https://wiki.php.net/rfc/pow-operator\">Power Operator</ulink>"
msgstr ""

#. Tag: para
