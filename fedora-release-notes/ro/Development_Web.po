#, no-c-format
msgid "Security"
msgstr ""

#. Tag: para
#, no-c-format
msgid ""
"<ulink url=\"https://wiki.php.net/rfc/tls-peer-verification\">TLS Peer "
"Verification</ulink>"
msgstr ""

#. Tag: para
#, no-c-format
msgid ""
"<ulink url=\"https://wiki.php.net/rfc/improved-tls-defaults\">Improved TLS "
"Defaults</ulink>"
msgstr ""

#. Tag: title
#, no-c-format
msgid "Language improvements"
msgstr ""

#. Tag: para
#, no-c-format
msgid ""
"<ulink url=\"https://wiki.php.net/rfc/const_scalar_exprs\">Constant Scalar "
